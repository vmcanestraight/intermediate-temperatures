axes.linewidth: 1.0

axes.spines.top: True
axes.spines.right: True
axes.prop_cycle: cycler('color', ["#2c457d","#50c5db","#fe8f06","#4b8163","#e30291","#c6c0fe","#48006a","#e60027","#c0e087"])